    "#En las siguientes lineas se hará la comparación entre las bases de datos.\n",
    "Origin=pd.merge(Observaciones,Efemerides,how='left')\n",
    "Origin = Origin[['Año','Mes','Día','Mag','Delta','r','Phase','Filtro']]\n",
    "Origin"
