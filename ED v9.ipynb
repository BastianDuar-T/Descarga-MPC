   "source": [
    "#Función para descargar efemerides de un objeto\n",
    "\n",
    "def efemerides(objeto,fecha,numero):\n",
    "    eph = MPC.get_ephemeris(objeto, start=fecha, step='1d', number=numero)\n",
    "    eph.add_column(eph['Date'].value,name='F')\n",
    "    tabla=eph.as_array(names=['Delta','r','Phase','F'])\n",
    "    ef=pd.DataFrame(tabla.data)\n",
    "    ef['Año']=ef['F'].str[0:4]\n",
    "    ef['Mes']=ef['F'].str[5:7]\n",
    "    ef['Día']=ef['F'].str[8:10]\n",
    "    ef[['Año','Mes','Día']]=ef[['Año','Mes','Día']].astype(int)\n",
    "    ef['Fecha']=10000*ef['Año']+100*ef['Mes']+ef['Día']\n",
    "    ef = ef[['Fecha','Delta','r','Phase']]\n",
    "    return ef\n",
    "    "
   ]
  },
