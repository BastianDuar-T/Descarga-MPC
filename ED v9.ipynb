    "#codigo para cargar observaciones desde el MPC,filtrar aquellas que tengan datos vacios en la columna Mag\n",
    "# y hacer un Id con la fecha\n",
    "\n",
    "Obj=input('Número del objeto ')\n",
    "\n",
    "obs = MPC.get_observations(Obj,get_mpcformat=True)\n",
    "df=obs.to_pandas()\n",
    "\n",
    "df['Año']=df['obs'].str[15:20]\n",
    "df['Mes']=df['obs'].str[20:23]\n",
    "df['Día']=df['obs'].str[23:25]\n",
    "df['Mag']=pd.to_numeric(df['obs'].str[65:70],errors='coerce')\n",
    "df['Filtro']=df['obs'].str[70:71]\n",
    "df[['Año','Mes','Día']]=df[['Año','Mes','Día']].astype(int)\n",
    "df=df.dropna(subset=['Mag'])\n",
    "df['Fecha']=10000*df['Año']+100*df['Mes']+df['Día']\n",
    "Observaciones = df[['Fecha','Año','Mes','Día','Mag','Filtro']]\n",
    "Observaciones"
   ]
  },
