   "source": [
    "Origin['Frac año']=((Origin['Día']/31)+Origin['Mes']-1)/12+Origin['Año']\n",
    "Origin['T-Tq']=0\n",
    "q=((d/31.0)+m-1)/12+a\n",
    "# Se crea una lista con las fechas de perihelios que ha tenído el objeto desde su primera fecha de observación\n",
    "Q=[q] \n",
    "# En las siguientes lineas se hace un ciclo para calcular los perihelios\n",
    "while q>=Origin.loc[0,'Frac año']:\n",
    "    q=q-P\n",
    "    Q=np.insert(Q,0,q)\n",
    "\n",
    "# Con el ciclo que sigue se determina la diferencia en días que hay entre la fecha de observación y el perihelio más cercano \n",
    "for e in Q:\n",
    "    a=e-p\n",
    "    b=e+p\n",
    "    for i in range(0,len(Origin)):\n",
    "        if Origin.loc[i,'Frac año']>=a and Origin.loc[i,'Frac año']<b:\n",
    "            Origin.loc[i,'T-Tq']=(Origin.loc[i,'Frac año']-e)*365.25\n",
    "# Se Ordena de menor T-Tq a mayor\n",
    "Origin = Origin.sort_values('T-Tq')\n",
    "#filtrado por fase\n",
    "Origin=Origin[Origin['Phase']>=5]\n",
    "Origin"
   ]
