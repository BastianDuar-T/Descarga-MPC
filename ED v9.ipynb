    "#codigo para calcular número de días entre primera y última fecha de observación\n",
    "\n",
    "f1=dt.date(df.iloc[0,1], df.iloc[0,2], df.iloc[0,3])\n",
    "f2=dt.date(df.iloc[-1,1], df.iloc[-1,2], df.iloc[-1,3])\n",
    "delta=(f2-f1).days\n",
    "\n",
    "#Ciclo que permite descargar efemerides teniendo en cuenta una fecha inicial y una final\n",
    "Efemerides=pd.DataFrame()\n",
    "while delta>1400:\n",
    "    start=f1.isoformat()\n",
    "    f1=f1+dt.timedelta(days=1400)\n",
    "    delta=delta-1400\n",
    "    d=1400\n",
    "    ef=efemerides(Obj,start,d)\n",
    "    Efemerides=pd.concat([Efemerides, ef], axis=0,ignore_index=True)\n",
    "else:\n",
    "    end=f1.isoformat()\n",
    "    delta=delta+1\n",
    "    ef=efemerides(Obj,end,delta)\n",
    "    Efemerides=pd.concat([Efemerides, ef], axis=0,ignore_index=True)\n",
    "\n",
    "Efemerides"
   ]
